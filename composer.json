{
    "name": "janvernieuwe/dev-branch-check",
    "description": "Checks for forgotten dev- branches in composer",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Janvernieuwe\\DevBranchCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Janvernieuwe\\DevBranchCheck\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jan Vernieuwe",
            "email": "jan.vernieuwe@phpro.be"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/options-resolver": "^6.3 | ^7.0",
        "phpro/grumphp": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0 | ~10.0",
        "friendsofphp/php-cs-fixer": "^3.51"
    }
}
